   "source": [
    "import numpy as py\n",
    "import pandas as pd\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.metrics import r2_score\n",
    "from sklearn.neural_network import MLPRegressor\n",
    "from sklearn.linear_model import LinearRegression\n",
    "import time\n",
    "import pandas as pd\n",
    "\n",
    "# Reading the dataset\n",
    "\n",
    "data = pd.read_csv('house_price.csv')\n",
    "data.head()\n",
    "data = data.iloc[:,1:]\n",
    "label_enc = LabelEncoder()\n",
    "data.iloc[:,0] = label_enc.fit_transform(data.iloc[:,0])\n",
    "data.head()\n",
    "data=data.drop([\"Location\"],axis=1)\n",
    "X = data.drop('Price', axis=1)\n",
    "y = data['Price']\n",
    "data[\"Old(years)\"].unique()\n",
    "\n",
    "#Training the data\n",
    "\n",
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.2, random_state = 1)\n",
    "start = time.time()\n",
    "\n",
    "#Applied ML algorithm ffo training the data\n",
    "\n",
    "mlp = MLPRegressor(hidden_layer_sizes=(100,100,100), activation=\"relu\", batch_size=10, learning_rate_init=0.001, max_iter=400, random_state=1)\n",
    "mlp.fit(X_train, y_train)\n",
    "end = time.time()\n",
    "print(end-start)\n",
    "modeling=LinearRegression()\n",
    "modeling.fit(X_train,y_train)\n",
    "pred = mlp.predict(X_test)\n",
    "r2_score(y_test, pred)\n",
    "predictions=modeling.predict(X_test)\n",
    "r2_score(y_test, predictions)\n",
    "\n",
    "# Us based house price Prediction\n",
    "\n",
    "BHK=int(input(\"Enter Number of Bedrooms: \"))\n",
    "\n",
    "\n",
    "Furnishing=int(input(\"Enter with furniture or not...1 or 0:  \"))\n",
    "\n",
    "\n",
    "Sq_feet=int(input(\"Enter Area in Square feet:  \"))\n",
    "\n",
    "\n",
    "Old_Years=int(input(\"Enter How many years old:  \"))\n",
    "\n",
    "\n",
    "Floor=int(input(\"Enter in which floor: \"))\n",
    "\n",
    "#printing the predicted value\n",
    "\n",
    "print(modeling.predict([[BHK,Furnishing,Sq_feet,Old_Years,Floor]]),'Dollars')"
   ]
